{
  "name": "Surpass",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Secure, unobtrusive, rapid password management",
  "permissions": ["tabs"],
  "background": { "scripts": ["jquery-1.7.2.min.js","jquery.sha256.js", "background.js" ] },
  "options_page": "options.html",
  "page_action": {
     "default_name": "Surpass",
      "default_icon": "icon-19.png"
  },
   "icons" : {
    "48" : "icon-48.png",
    "128" : "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-1.7.2.min.js", "jquery.sha256.js", "content.js"],
      "css": ["surpass.css"]
    }
   ] 
}

